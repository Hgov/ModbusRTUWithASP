@page
@model WebApiModel
@{
    ViewData["Title"] = "Web API";
    ViewData["Header1"] = "The Web API documentation page.";
}

<h2>@ViewData["Title"]</h2>
<h3>@ViewData["Header1"]</h3>

<style>
    iframe {
        border: 0;
        background: #ffffff;
        position: fixed;
        left: 0;
        right: 0;
        width: 100%;
        height: 75%;
    }
</style>

<div id="SwaggerRegion" class="embed-responsive embed-responsive-16by9">
    <iframe class="embed-responsive-item" src="/swagger" scrolling="yes" allowfullscreen>
        Your browser does not support iframes.
    </iframe>
</div>